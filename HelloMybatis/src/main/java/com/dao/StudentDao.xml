<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.StudentDao">
    <select id="selectStudent" resultType="com.domain.Student">

       select * from student
    </select>

    <insert id="insertStudent" >
        insert into student(`name`,sex,age,className) values(#{name},#{sex},#{age},#{className})
    </insert>
    <select id="select3" resultType="com.domain.Student">
        select * from student where sex=#{sex} and age=#{age}

    </select>

</mapper>